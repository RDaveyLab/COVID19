require(pROC)
all_calc = function(predicted, ground_truth){
AUC = auc(predicted ~ ground_truth)
XX = table(predicted, ground_truth)
tn = XX[1,1]
tp = XX[2,2]
fp = XX[2,1]
fn = XX[1,2]
TPR = tp/(tn + tp)
TNR = tn/(tn + tp)
PPV = tp/(tp + fp)
NPV = tn /(tn+fn)
FNR = fn/(fn+tp)
FPR = fp/(fp + tn)
FDR = fp/(fp + tp)
ACC = (tp + tn)/(fp + tp + tn + fn)
F1 = (tp*2)/(2*tp + fp +fn)
O = data.frame(AUC = AUC,
TN = tn,
TP = tp,
FP = fp,
FN = fn,
TPR = TPR,
TNR = TNR,
PPV = PPV,
NPV = NPV,
FNR = FNR,
FPR = FPR,
FDR = FDR,
ACC = ACC,
F1 = F1
)
return(O)
}
Low = all_calc(O2$Low_Model , O2$Low_GT)
